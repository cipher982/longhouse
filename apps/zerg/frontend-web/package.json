{
  "name": "zerg-frontend-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "node ./scripts/run-vitest.mjs",
    "generate:api": "openapi-typescript ../openapi.json --output src/generated/openapi-types.ts",
    "validate:contracts": "bun ./scripts/contract-validator.ts",
